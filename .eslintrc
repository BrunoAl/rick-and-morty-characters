{
  "extends": ["eslint:recommended", "plugin:react/recommended", "kentcdodds", "plugin:jsx-a11y/recommended"],
  "rules": {
    "no-unused-vars": ["warn"],
    "no-console": ["warn"],
    "react/prop-types": ["warn"],
    "no-use-before-define": "off",
    "jsx-a11y/anchor-is-valid": ["warn"]
  },
  "plugins": ["jsx-a11y"],
  "env": {
    "jest": true
  }
}
